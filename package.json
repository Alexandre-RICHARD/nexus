{
  "type": "module",
  "scripts": {
    " === Common Test === ": "",
    "global-test": "pnpm check-types && pnpm lint-fix && pnpm unit-test-once",
    "unit-test-watch": "vitest watch",
    "unit-test-once": "vitest run",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    " === Pnpm === ": "",
    "install-dependencies": "pnpm i",
    "update-dependencies": "pnpm update",
    " === Build === ": "",
    "unit-test-report": "pnpm clean && vitest run --reporter=html --coverage.enabled=true && vite preview --outDir report --open",
    "clean": "rd /s /q build report || rm -rf build report",
    "clean:all": "rd /s /q build node_modules report || rm -rf build node_modules report"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.10.1",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vitest": "0.4.1",
    "jsdom": "^24.1.3",
    "prettier": "^3.3.3",
    "sass": "^1.80.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
