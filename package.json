{
  "type": "module",
  "scripts": {
    "global-test": "pnpm clean && pnpm lint-fix && pnpm check-types && vitest run",
    "test-unit": "vitest --mode dev",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
