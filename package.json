{
  "type": "module",
  "scripts": {
    " === Common Test === ": "",
    "global-test": "pnpm check-types && pnpm lint-fix && pnpm unit-test-once",
    "unit-test-watch": "vitest watch",
    "unit-test-once": "vitest run",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    " === Pnpm === ": "",
    "install-dependencies": "pnpm i",
    "update-dependencies": "pnpm update",
    " === Build === ": "",
    "unit-test-report": "pnpm clean && vitest run --reporter=html --coverage.enabled=true && vite preview --outDir report --open",
    "clean": "rd /s /q report || rm -rf report",
    "clean:all": "rd /s /q node_modules report || rm -rf node_modules report"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.12.0",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vitest": "0.4.1",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
